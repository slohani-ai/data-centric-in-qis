    "import Partial_Trace as pt\n",
    "import qiskit.quantum_info as qi\n",
    "\n",
    "rho = qi.random_density_matrix(dims=2, rank=1).tensor(qi.random_density_matrix(dims=2, rank=1))\n",
    "print(rho.purity())\n",
    "rho_d = rho.data.reshape(1, 4, 4)\n",
    "print(rho_d)\n",
    "print(pt.find(rho.data))\n",
    "pt.PPT(rho_d)"
   ]
  }
